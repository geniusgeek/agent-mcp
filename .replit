task = "shell.exec"
args = "python -c \"import sys; sys.path.append('.'); from autonomous_langgraph_network import AutonomousAgentNetwork; network = AutonomousAgentNetwork(); network.create_network(); print('=== Starting Research on MCP and future of agentic work ==='); network.research_topic('MCP and future of agentic work', max_steps=10)\""

