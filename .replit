modules = ["python-3.11"]

[nix]
channel = "stable-24_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "MCPAgentTest"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "mcp_agent_test"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "AgentNetwork"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "CollaborativeProject"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "MCPFeaturesDemo"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "LangGraphExample"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "LangGraphAgentNetwork"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "LangGraphCollaborativeTask"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "AgentCollaboration"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "AutonomousLangGraphNetwork"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "AutonomousResearch"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "DetailedAgentInteractions"

